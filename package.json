{
  "version": "0.3.5",
  "license": "MIT",
  "type": "module",
  "main": "dist/appmint-form.cjs.js",
  "module": "dist/appmint-form.es.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src/library",
    "src/index.tsx",
    "src/index.css",
    "src/types"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/appmint-form.es.js",
      "require": "./dist/appmint-form.cjs.js"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "vite build --config vite.config.mjs --watch",
    "build": "vite build --config vite.config.mjs",
    "test": "vitest run --passWithNoTests",
    "lint": "echo \"ESLint disabled for this project\"",
    "prepare": "vite build --config vite.config.mjs",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "dev": "vite --config vite.demo.config.ts",
    "demo": "vite --config vite.demo.config.ts",
    "build:demo": "vite build --config vite.demo.config.ts",
    "theme-test": "vite --config vite.theme-test.config.ts",
    "build:theme-test": "vite build --config vite.theme-test.config.ts"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo \"ESLint disabled for this project\""
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@appmint/form",
  "author": "Jac Light",
  "size-limit": [
    {
      "path": "dist/appmint-form.cjs.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/appmint-form.es.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@size-limit/preset-small-lib": "^11.1.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react": "^7.37.4",
    "husky": "^9.0.11",
    "jsdom": "^26.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "size-limit": "^11.1.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-lib-inject-css": "^2.2.1",
    "vitest": "^3.0.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/durubata/appmint-form.git"
  },
  "homepage": "https://github.com/durubata/appmint-form",
  "dependencies": {
    "@radix-ui/react-select": "^2.1.6",
    "@tailwindcss/vite": "^4.0.9",
    "@tanstack/match-sorter-utils": "^8.15.1",
    "@tanstack/react-table": "^8.19.2",
    "@uiw/react-md-editor": "^4.0.4",
    "axios": "^1.7.2",
    "color": "^5.0.0",
    "colord": "^2.9.3",
    "deep-object-diff": "^1.1.9",
    "generate-schema": "^2.6.0",
    "immer": "^10.1.1",
    "lucide-react": "^0.476.0",
    "object-path": "^0.11.8",
    "react-best-gradient-color-picker": "^3.0.14",
    "react-json-view": "^1.21.3",
    "tailwind-merge": "^3.0.2",
    "uuid": "^10.0.0",
    "zod": "^3.24.2",
    "zustand": "5.0.2"
  },
  "packageManager": "yarn@4.7.0"
}
